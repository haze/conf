{
  "languageserver": {
    "lisp": {
      "command": "cl-lsp",
      "filetypes": [
        "lisp"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "haskell": {
      "command": "hie-wrapper",
      "args": [
        "--lsp"
      ],
      "rootPatterns": [
        "stack.yaml",
        "cabal.config",
        "package.yaml"
      ],
      "filetypes": [
        "hs",
        "lhs",
        "haskell"
      ],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "zig": {
      "command": "/Users/haze/src/zls/zig-cache/bin/zls",
      "filetypes": [
        "zig"
      ]
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": [
        "nim"
      ]
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    },
    "kotlin": {
      "command": "/Users/haze/std/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "trace.server": "verbose",
      "filetypes": [
        "kotlin",
        "kt"
      ]
    }
  },
  "suggest.noselect": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.timeout": 500,
  "diagnostic.errorSign": "ER",
  "diagnostic.warningSign": "WN",
  "diagnostic.infoSign": "IN",
  "diagnostic.hintSign": ":)",
  "rust-analyzer.trace.server": "verbose",
  "java.format.comments.enabled": true,
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.completion.overwrite": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "java",
    "css",
    "go",
    "ocaml",
    "python",
    "markdown",
    "typescript",
    "json",
    "javascript",
    "svelte",
    "rust",
    "sql",
    "jsonc"
  ],
  "html.format.enable": true,
  "rust-analyzer.inlayHints.chainingHints": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.format.onType.enabled": false
}
