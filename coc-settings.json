{
  "languageserver": {
    "lisp": {
      "command": "cl-lsp",
      "filetypes": [
        "lisp"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": [
        "nim"
      ]
    },
    "zig": {
      "module": "/Users/haze/src/zig-language-server/server/out/server.js",
      "args": [
        "--node-ipc"
      ],
      "filetypes": [
        "zig"
      ],
      "trace.server": "verbose",
      "rootPatterns": [
        "src/"
      ],
      "execArgv": [
        "--nolazy",
        "--inspect-brk=6045"
      ],
      "initializationOptions": {},
      "settings": {
        "validate": true
      }
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    },
    "kotlin": {
      "command": "/Users/haze/std/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "trace.server": "verbose",
      "filetypes": [
        "kotlin",
        "kt"
      ]
    }
  },
  "suggest.noselect": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.timeout": 500,
  "diagnostic.errorSign": "ER",
  "diagnostic.warningSign": "WN",
  "diagnostic.infoSign": "IN",
  "diagnostic.hintSign": ":)",
  "rust-analyzer.trace.server": "verbose",
  "java.format.comments.enabled": true,
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.completion.overwrite": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "java",
    "css",
    "go",
    "ocaml",
    "python",
    "markdown",
    "typescript",
    "json",
    "javascript",
    "svelte",
    "rust",
    "sql",
    "jsonc"
  ],
  "tabnine.shortcut": "ML",
  "tabnine.priority": 0,
  "tabnine.disable_filetypes": [
    "java"
  ],
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.format.onType.enabled": false
}
