{
  "languageserver": {
    "lisp": {
      "command": "cl-lsp",
      "filetypes": [
        "lisp"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "zig": {
      "enable": true,
      "command": "/Users/haze/src/ziglsp/zig-cache/bin/ziglsp",
      "filetypes": [
        "zig"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "ocaml": {
      "command": "ocaml-language-server",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    },
    "kotlin": {
      "command": "/Users/haze/std/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "trace.server": "verbose",
      "filetypes": [
        "kotlin",
        "kt"
      ]
    }
  },
  "suggest.noselect": false,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.timeout": 500,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "?",
  "diagnostic.infoSign": "⚑",
  "diagnostic.hintSign": "⚑",
  "svelte.plugin.typescript.diagnostics.enable": false,
  "tsserver.formatOnType": false,
  "java.format.onType.enabled": false,
  "rust-analyzer.enableCargoWatchOnStartup": "enabled",
  "coc.preferences.formatOnSaveFiletypes": [
    "java",
    "css",
    "go",
    "ocaml",
    "python",
    "markdown",
    "typescript",
    "json",
    "javascript",
    "svelte",
    "rust",
    "sql",
    "jsonc"
  ],
  "tabnine.shortcut": "ML",
  "tabnine.priority": 0,
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.format.onType.enabled": false,
  "go.trace.server": "verbose"
}
